@'
# Maven
target/

# Build artifacts (don’t commit)
*.jar
*.war
*.zip

# Keep Maven wrapper if you use it
!.mvn/wrapper/maven-wrapper.jar

# IDE
.idea/
.vscode/
*.iml

# OS noise
.DS_Store
Thumbs.db
